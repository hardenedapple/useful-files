! vimcmd: !xrdb -load ~/xresources/base_resources
! vimcmd: !xrdb -merge ~/xresources/own_colors
UXTerm*termName:   xterm-256color
! I like the terminal font more, but it doesnt have a lot of utf characters
! font  -> default
! font1 -> unreadable
! font2 -> tiny
! font3 -> small
! font4 -> medium
! font5 -> large
! font6 -> huge
UXTerm*faceName:       Liberation Mono
UXTerm*faceSize:       8
UXTerm*faceSize1:       4
UXTerm*faceSize2:       6
UXTerm*faceSize3:       8
UXTerm*faceSize4:       10
UXTerm*faceSize5:       11
UXTerm*faceSize6:       14
UXTerm*faceSize7:       16
UXTerm*renderFont:     true
UXTerm*iconName:   Apples_Terminal
UXTerm*title:      Apples_Terminal
UXTerm*utf8:       2
UXTerm*saveLines:  4096
UXTerm*omitTranslation: fullscreen
!UXTerm*locale: true
UXTerm*VT100.translations: #override \
    <Btn1Up>: select-end(PRIMARY, CLIPBOARD, CUT_BUFFER0) \n\
    Ctrl<Key>Return: spawn-new-terminal()

UXTerm*disallowedWindowOps: 20,21,SetXprop

! Doesnt seem to do anything - will check later on
! UXTerm*VT100.Translations: #override \
!             <Key>Backspace: string(0x7F)\n\
!             <Key>Delete:    string("\033[3~")\n\
!             <Key>Home:      string("\033[1~")\n\
!             <Key>End:       string("\033[4~")

! Change xterm specific keys - e.g. ctrl-h == backspace
! This allows setting ctrl-h to something in tmux
UXTerm*ttyModes:  erase ^?
UXTerm*backarrowKeyIsErase: true

UXTerm*metaSendsEscape: true

UXTerm*scrollKey: true

